{
    "manifest_version" : 2,
    "name" : "RapidNote",
    "description" : "A simple, hustle free note taking extension for Google Chrome",
    "version" : "1.1",
    "browser_action" : {
        "default_title" : "RapidNote",
        "default_icon" : "dist/assets/icon.png",
        "default_popup" : "./dist/popup.html"
     },
     "icons" : {
         "16" : "dist/assets/icon16.png",
         "48" : "dist/assets/icon48.png",
         "128" : "dist/assets/icon128.png"
     }
}
